    <slot-configuration slot-id="home-promo" context="global" configuration-id="overlapping-home-promo" assigned-to-site="true">
        <description>Overlapping home promotion</description>
        <template>slots/content/rendermoduletemplate.isml</template>
        <enabled-flag>true</enabled-flag>
        <content>
            <content-assets>
                <content-asset content-id="home-promo-overlapping"/>
            </content-assets>
        </content>
    </slot-configuration>

