<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.demandware.com/xml/impex/library/2006-10-31">
  <content content-id="footer-newsletter-subscription">
    <display-name xml:lang="x-default">Footer Newsletter Subscription</display-name>
    <online-flag>true</online-flag>
    <searchable-flag>false</searchable-flag>
    <custom-attributes>
      <custom-attribute attribute-id="body" xml:lang="x-default">&lt;div class=&quot;envelope section-image&quot;&gt;
        &lt;img alt=&quot;&quot; src=&quot;images/footer/np_envelope.png?$staticlink$&quot; title=&quot;&quot; /&gt;
        &lt;/div&gt;
        &lt;div class=&quot;section-title&quot;&gt;Newsletter Subscription&lt;/div&gt;
        &lt;div class=&quot;section-text&quot;&gt;Sign up to&amp;nbsp;be first to know about sales, events &amp;amp; new arrivals.&lt;/div&gt;
        &lt;form class=&quot;newsletter-subscription&quot; id=&quot;newsletter-subscription&quot;&gt;
        &lt;div class=&quot;newsletter-input&quot;&gt;
        &lt;label class=&quot;empty-link-text&quot; for=&quot;emailsubscription&quot;&gt;Email  subscription&lt;/label&gt;
        &lt;input id=&quot;emailsubscription&quot; class=&quot;required&quot; name=&quot;emailsubscription&quot; placeholder=&quot;Your email, please&quot; type=&quot;email&quot; /&gt;
        &lt;span id=&quot;email-subscription-message&quot;&gt;&lt;/span&gt;
        &lt;/div&gt;
        &lt;div class=&quot;newsletter-cta&quot;&gt;
        &lt;button class=&quot;footer-email-subscribtion&quot; type=&quot;button&quot;&gt;
        &lt;img alt=&quot;Submit&quot; src=&quot;images/footer/cta_smallarrow_right.png?$staticlink$&quot; /&gt;
        &lt;/button&gt;
        &lt;/div&gt;
        &lt;div class=&quot;form-row label-inline agree-casl-ckeckbox agree-casl-container&quot;&gt;
        &lt;div class=&quot;field-wrapper&quot;&gt;
        &lt;input id=&quot;agree-casl&quot; class=&quot;agree-casl&quot; name=&quot;subscribe&quot; type=&quot;checkbox&quot; value=&quot;newsletter&quot;/&gt;
        &lt;/div&gt;
        &lt;label for=&quot;agree-casl&quot;&gt;Zwilling'den kişiye özel ticari elektronik iletilerini (sms ve e-posta) almak için açık onay veriyorum.&lt;/label&gt;
        &lt;/div&gt;
        &lt;div class=&quot;form-row label-inline agree-casl-ckeckbox agree-privacies-container&quot;&gt;
        &lt;div class=&quot;field-wrapper&quot;&gt;
        &lt;input id=&quot;agree-privacies&quot; class=&quot;agree-privacies&quot; name=&quot;agree-privacies&quot; type=&quot;checkbox&quot; value=&quot;agree-privacies&quot;/&gt;
        &lt;/div&gt;
        $include(Page-Include, cid, signup-policy-terms)$
        &lt;/div&gt;
        &lt;div class=&quot;form-row label-inline agree-casl-ckeckbox clear-consent-container&quot;&gt;
        &lt;div class=&quot;field-wrapper&quot;&gt;
        &lt;input id=&quot;clear-consent&quot; class=&quot;clear-consent&quot; name=&quot;clear-consent&quot; type=&quot;checkbox&quot; value=&quot;clear-consent&quot;/&gt;
        &lt;/div&gt;
        $include(Page-Include, cid, clear-consent)$
        &lt;/div&gt;
        &lt;/form&gt;</custom-attribute>
      </custom-attributes>
    </content>
  </library>